nav(class="container pt-0")
  div(class="row")
    div(class="col-md-6")
      p Eastern conference
      ul
        each val, index in teamsList
          if val.conference == 'East'
            li
              <a href="/teams/#{val.slug}">#{val.name}</a>
    div(class="col-md-6")
      p Western conference
      ul
        each val, index in teamsList
          if val.conference == 'West'
            li
              <a href="/teams/#{val.slug}">#{val.name}</a>
